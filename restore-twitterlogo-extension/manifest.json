{
  "manifest_version": 2,
  "name": "Twitterのロゴ戻すやーつ",
  "version": "1.0",
  "description": "青い鳥を蘇生するスクリプト",
  "icons": {
    "48": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "https://twitter.com/*",
    "https://x.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*", "https://x.com/*"],
      "js": ["background.js"]
    }
  ]
}
